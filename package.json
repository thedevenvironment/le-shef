{
  "scripts": {
    "build:android": "npx eas-cli build --platform android",
    "build:ios": "npx eas-cli build --platform ios --local",
    "build:web": "expo export -p web --output-dir web-build --dump-sourcemap",
    "dev": "npm start",
    "lint": "eslint --ext .tsx,.ts ./",
    "prettier": "prettier --write '**/*.{ts,tsx,json,md}'",
    "serve:app": "expo start --dev-client",
    "serve:web": "npx serve web-build",
    "start": "expo start",
    "upgrade": "expo upgrade"
  },
  "overrides": {
    "json5": "2.2.2"
  },
  "dependencies": {
    "expo": "^48.0.4",
    "expo-constants": "~14.2.1",
    "expo-linking": "~4.0.1",
    "expo-router": "^1.2.0",
    "expo-splash-screen": "~0.18.1",
    "expo-status-bar": "~1.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.3",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-web": "~0.18.7",
    "styled-components": "^5.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@types/react": "~18.0.27",
    "@types/styled-components-react-native": "^5.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.4",
    "typescript": "^4.9.4"
  }
}
